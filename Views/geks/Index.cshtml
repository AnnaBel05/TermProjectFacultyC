@model IEnumerable<TermProjectFacultyC.gek>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            <div>Фамилия</div>
        </th>
        <th>
            <div>Имя</div>
        </th>
        <th>
            <div>Отчество</div>
        </th>
        <th>
            <div>Место работы</div>
        </th>
        <th>
            <div>Должность</div>
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.lastname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.firstname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.patronymic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.placeofwork)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.position)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>

@if (User.Identity.IsAuthenticated)
{
    <p>
        <button>@Html.ActionLink("Шаги", "GradWorkTest", "Home")</button>
    </p>
    <p>
        <button>@Html.ActionLink("Следующий шаг", "Index", "eventlogs")</button>
    </p>
}
