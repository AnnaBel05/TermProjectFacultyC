@model IEnumerable<TermProjectFacultyC.eventlog>

@{
    ViewBag.Title = "Информация о защитах";
}

<h2>Информация о защитах</h2>

<p>
    @Html.ActionLink("Создать новый", "Создать")
</p>
<table class="table">
    <tr>
        <th>
            <div> Дата защиты </div>
        </th>
        <th>
            <div> Аудитория </div>
        </th>
        <th>
            <div> Ответственный преподаватель </div>
        </th>
        <th>
            <div> Описание мероприятия </div>
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.eventdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.eventplace)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.responsibleprofessor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.eventdescr)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>

@if (User.Identity.IsAuthenticated)
{
    <p>
        <button>@Html.ActionLink("Шаги", "GradWorkTest", "Home")</button>
    </p>
    <p>
        <button>@Html.ActionLink("Следующий шаг", "Index", "geklineups")</button>
    </p>
    <p>
        <button>@Html.ActionLink("Предыдущий шаг", "Index", "geks")</button>
    </p>
}
